{
    "builds": [{
        "src": "AlgoViz/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static_build",
      "config": {
	"distDir": "staticfiles"
      }
    }
    ],
    "routes": [
      {
	"src": "/(.*)",
	"dest": "vercel_app/wsgi.py"
      },
      {
	"src": "/static/(.*)",
	"dest": "/static/$1"
      }
    ]

}
